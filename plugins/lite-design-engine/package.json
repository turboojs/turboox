{
  "name": "@turbox3d/lite-design-engine",
  "version": "1.0.0",
  "description": "lite design engine",
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "types": "./index.d.ts",
  "keywords": [
    "front-end",
    "turbox3d",
    "turbox",
    "alibaba",
    "taobao",
    "lite-design-engine"
  ],
  "tnpm": {
    "mode": "yarn"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "author": "feifan <darknight3@vip.qq.com>",
  "license": "MIT",
  "peerDependencies": {
    "@turbox3d/turbox": ">=1",
    "pixi.js": ">=5.3",
    "react": "16.x",
    "react-dom": "16.x",
    "three": "0.x"
  },
  "devDependencies": {
    "@turbox3d/turbox-dev-tool": "^1.0.9",
    "@turbox3d/turbox": "^2.0.4",
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-transform-modules-commonjs": "^7.6.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/runtime": "^7.6.3",
    "@types/jest": "^24.0.19",
    "@types/node": "^12.11.5",
    "@types/react": "16.x",
    "@types/react-dom": "16.x",
    "@types/three": "^0.130.1",
    "antd": "^4.16.12",
    "copyfiles": "^2.4.1",
    "cross-env": "^6.0.3",
    "file-loader": "^6.2.0",
    "jest": "^24.9.0",
    "pixi.js": "^6.1.3",
    "react": "16.x",
    "react-dom": "16.x",
    "three": "^0.132.2",
    "ts-jest": "^24.1.0",
    "typescript": "^4.3.5",
    "url-loader": "^4.1.1",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2"
  },
  "files": [
    "src/",
    "lib/",
    "es/",
    "index.d.ts",
    "typings"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/turbox3d/turbox.git"
  },
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.dev.js",
    "test": "npx jest --forceExit --coverage --verbose --passWithNoTests",
    "test:watch": "npm run test -- --watchAll",
    "build:ts": "rm -rf ts && tsc -target es2018 -declarationDir ./typings -outDir ./ts",
    "build:es": "rm -rf es && npm run build:ts && npx babel ts --out-dir ./es",
    "build:lib": "rm -rf lib && npm run build:ts && cross-env BABEL_ENV=commonjs npx babel ts --out-dir lib",
    "build:watch": "tsc -w -target es2018 -declarationDir ./typings -outDir ./ts & npx babel ts -w --out-dir es",
    "build:umd": "npx webpack --mode='development' -o dist/lite-design-engine.js",
    "build:umd:min": "npx webpack --mode='production' -o dist/lite-design-engine.min.js",
    "build:all": "npm run build:es && npm run build:lib && npm run build:umd && npm run build:umd:min",
    "prepublishOnly": "npm run build:all"
  },
  "dependencies": {
    "pixi-projection": "^0.4.3"
  }
}
