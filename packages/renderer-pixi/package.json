{
  "name": "@turbox3d/renderer-pixi",
  "version": "1.0.13",
  "description": "Large-scale declarative 2d graphic ui framework based on pixi",
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "types": "./index.d.ts",
  "keywords": [
    "framework",
    "front-end",
    "turbox3d",
    "turbox",
    "alibaba",
    "taobao",
    "declarative-graphic-ui",
    "pixi"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "author": "feifan <darknight3@vip.qq.com>",
  "license": "MIT",
  "peerDependencies": {
    "pixi.js": ">=5.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.5",
    "@babel/plugin-transform-modules-commonjs": "^7.23.3",
    "@babel/plugin-transform-runtime": "^7.23.4",
    "@babel/preset-env": "^7.23.5",
    "@babel/runtime": "^7.23.5",
    "@types/jest": "^24.9.1",
    "@types/node": "~16.18.68",
    "cross-env": "^6.0.3",
    "jest": "^24.9.0",
    "pixi.js": "^5.3.12",
    "ts-jest": "^24.3.0",
    "typescript": "4.6.4",
    "webpack": "4.42.1",
    "webpack-cli": "~3.3.12"
  },
  "files": [
    "src/",
    "lib/",
    "es/",
    "index.d.ts",
    "typings"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/turbox3d/turbox.git"
  },
  "scripts": {
    "test": "npx jest --forceExit --coverage --verbose --passWithNoTests",
    "test:watch": "npm run test -- --watchAll",
    "build:ts": "rm -rf ts && tsc -target es2018 -declarationDir ./typings -outDir ./ts",
    "build:es": "rm -rf es && npm run build:ts && npx babel ts --out-dir ./es",
    "build:lib": "rm -rf lib && npm run build:ts && cross-env BABEL_ENV=commonjs npx babel ts --out-dir lib",
    "build:watch": "tsc -w -target es2018 -declarationDir ./typings -outDir ./ts & npx babel ts -w --out-dir es",
    "build:umd": "npx webpack --mode='development' -o dist/turbox-renderer-pixi.js",
    "build:umd:min": "npx webpack --mode='production' -o dist/turbox-renderer-pixi.min.js",
    "build:all": "npm run build:es && npm run build:lib && npm run build:umd && npm run build:umd:min"
  },
  "dependencies": {
    "@turbox3d/event-manager": "workspace:^",
    "@turbox3d/renderer-core": "workspace:^",
    "@turbox3d/shared": "workspace:^"
  }
}
