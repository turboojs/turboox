{
  "name": "@turbox3d/graphic-view",
  "version": "1.0.34",
  "description": "Large-scale declarative graphic ui core framework",
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "types": "./index.d.ts",
  "keywords": [
    "framework",
    "react",
    "front-end",
    "turbox3d",
    "turbox",
    "alibaba",
    "taobao",
    "declarative-graphic-ui"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "tnpm": {
    "mode": "npm"
  },
  "author": "feifan <darknight3@vip.qq.com>",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.14.0",
    "@babel/plugin-transform-modules-commonjs": "^7.14.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.14.1",
    "@babel/runtime": "^7.14.0",
    "@types/jest": "^24.0.19",
    "@types/node": "^12.20.12",
    "@types/react": "^16.14.6",
    "cross-env": "^6.0.3",
    "jest": "^24.9.0",
    "react": "^16.12.0",
    "ts-jest": "^24.1.0",
    "typescript": "^4.3.5",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.9"
  },
  "files": [
    "src/",
    "lib/",
    "es/",
    "index.d.ts",
    "typings"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/turbox3d/turbox.git"
  },
  "scripts": {
    "test": "npx jest --forceExit --coverage --verbose --passWithNoTests",
    "test:watch": "npm run test -- --watchAll",
    "build:ts": "rm -rf ts && tsc -target es2018 -declarationDir ./typings -outDir ./ts",
    "build:es": "rm -rf es && npm run build:ts && npx babel ts --out-dir ./es",
    "build:lib": "rm -rf lib && npm run build:ts && cross-env BABEL_ENV=commonjs npx babel ts --out-dir lib",
    "build:watch": "tsc -w -target es2018 -declarationDir ./typings -outDir ./ts & npx babel ts -w --out-dir es",
    "build:umd": "npx webpack --mode='development' -o dist/turbox-graphic-view.js",
    "build:umd:min": "npx webpack --mode='production' -o dist/turbox-graphic-view.min.js",
    "build:all": "npm run build:es && npm run build:lib && npm run build:umd && npm run build:umd:min",
    "prepublishOnly": "npm run build:all"
  },
  "dependencies": {
    "@turbox3d/command-manager": "^1.0.24",
    "@turbox3d/event-manager": "^1.0.30",
    "@turbox3d/reactivity": "^2.0.49",
    "@turbox3d/shared": "^1.0.11"
  }
}
