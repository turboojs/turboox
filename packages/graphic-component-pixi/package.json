{
  "name": "@turbox3d/graphic-component-pixi",
  "version": "1.1.24",
  "description": "Graphic component library based on pixi",
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "types": "./index.d.ts",
  "keywords": [
    "framework",
    "front-end",
    "turbox3d",
    "turbox",
    "alibaba",
    "taobao",
    "graphic-component-pixi"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "author": "feifan <darknight3@vip.qq.com>",
  "license": "MIT",
  "peerDependencies": {
    "pixi.js": ">=5.3",
    "three": "0.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.5",
    "@babel/plugin-transform-modules-commonjs": "^7.23.3",
    "@babel/plugin-transform-runtime": "^7.23.4",
    "@babel/preset-env": "^7.23.5",
    "@babel/runtime": "^7.23.5",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.20.55",
    "@types/three": "^0.130.2",
    "autoprefixer": "^10.4.16",
    "copyfiles": "^2.4.1",
    "cross-env": "^6.0.3",
    "css-loader": "^5.2.7",
    "file-loader": "^6.2.0",
    "jest": "^24.9.0",
    "less": "^4.2.0",
    "less-loader": "^7.3.0",
    "pixi.js": "^5.3.12",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^4.3.0",
    "sass-loader": "^10.4.1",
    "style-loader": "^2.0.0",
    "three": "^0.131.3",
    "ts-jest": "^24.3.0",
    "typescript": "4.3.5",
    "url-loader": "^4.1.1",
    "webpack": "4.42.1",
    "webpack-cli": "3.x"
  },
  "files": [
    "src/",
    "lib/",
    "es/",
    "index.d.ts",
    "typings"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/turbox3d/turbox.git"
  },
  "scripts": {
    "test": "npx jest --forceExit --coverage --verbose --passWithNoTests",
    "test:watch": "npm run test -- --watchAll",
    "build:ts": "rm -rf ts && tsc -outDir ./ts && copyfiles -u 1 src/**/*.less ./ts",
    "build:es": "rm -rf es && npm run build:ts && npx babel ts --out-dir ./es && copyfiles -u 1 src/**/*.less ./es",
    "build:lib": "rm -rf lib && npm run build:ts && cross-env BABEL_ENV=commonjs npx babel ts --out-dir lib && copyfiles -u 1 src/**/*.less ./lib",
    "build:watch": "tsc -w -outDir ./ts & npx babel ts -w --out-dir es & copyfiles -u 1 src/**/*.less ./es",
    "build:umd": "npx webpack --mode='development' -o dist/turbox-graphic-component-pixi.js",
    "build:umd:min": "npx webpack --mode='production' -o dist/turbox-graphic-component-pixi.min.js",
    "build:all": "npm run build:es && npm run build:lib && npm run build:umd && npm run build:umd:min",
    "prepublishOnly": "npm run build:all"
  },
  "dependencies": {
    "@turbox3d/event-manager": "workspace:^",
    "@turbox3d/renderer-pixi": "workspace:^",
    "@turbox3d/shared": "workspace:^"
  }
}
