{
  "name": "@turbox3d/reactivity-react",
  "version": "1.1.10",
  "description": "Large-scale reactive state management library for react",
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "types": "./index.d.ts",
  "keywords": [
    "framework",
    "react",
    "redux",
    "react-redux",
    "mobx",
    "mobx-react",
    "vuex",
    "front-end",
    "turbox3d",
    "turbox",
    "alibaba",
    "taobao",
    "state-management"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "author": "feifan <darknight3@vip.qq.com>",
  "license": "MIT",
  "peerDependencies": {
    "react": "16.x",
    "react-dom": "16.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.5",
    "@babel/plugin-transform-modules-commonjs": "^7.23.3",
    "@babel/plugin-transform-runtime": "^7.23.4",
    "@babel/preset-env": "^7.23.5",
    "@babel/runtime": "^7.23.5",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.20.55",
    "@types/react": "~16.14.52",
    "@types/react-dom": "~16.9.24",
    "@types/webpack-env": "^1.18.4",
    "cross-env": "^6.0.3",
    "jest": "^24.9.0",
    "react": "~16.14.0",
    "react-dom": "~16.14.0",
    "ts-jest": "^24.3.0",
    "typescript": "4.3.5",
    "webpack": "~5.89.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "@turbox3d/reactivity": "workspace:^",
    "@turbox3d/shared": "workspace:^"
  },
  "files": [
    "src/",
    "lib/",
    "es/",
    "index.d.ts",
    "typings"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/turbox3d/turbox.git"
  },
  "scripts": {
    "test": "npx jest --forceExit --coverage --verbose --passWithNoTests",
    "test:watch": "npm run test -- --watchAll",
    "build:ts": "rm -rf ts && tsc -target es2018 -declarationDir ./typings -outDir ./ts",
    "build:es": "rm -rf es && npm run build:ts && npx babel ts --out-dir ./es",
    "build:lib": "rm -rf lib && npm run build:ts && cross-env BABEL_ENV=commonjs npx babel ts --out-dir lib",
    "build:watch": "tsc -w -target es2018 -declarationDir ./typings -outDir ./ts & npx babel ts -w --out-dir es",
    "build:umd": "npx webpack --mode='development' -o dist/turbox-reactivity-react.js",
    "build:umd:min": "npx webpack --mode='production' -o dist/turbox-reactivity-react.min.js",
    "build:all": "npm run build:es && npm run build:lib && npm run build:umd && npm run build:umd:min",
    "prepublishOnly": "npm run build:all"
  },
  "bugs": {
    "url": "https://github.com/turbox3d/turbox/issues"
  }
}
