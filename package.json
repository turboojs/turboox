{
  "name": "turbox",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "scripts": {
    "start": "webpack-dev-server --config ./demo/webpack.config.js",
    "build": "NODE_ENV=production webpack ./demo/src/main --mode production --config ./demo/webpack.config.js",
    "lint-tsc": "tsc --noEmit",
    "build:doc": "npx typedoc packages/command-manager/src/index.ts packages/design-engine/src/index.ts packages/event-manager/src/index.ts packages/graphic-component-pixi/src/index.ts packages/graphic-component-three/src/index.ts packages/renderer-core/src/index.ts packages/renderer-pixi/src/index.ts packages/renderer-three/src/index.ts packages/math/src/index.ts packages/reactivity/src/index.ts packages/reactivity-react/src/index.ts packages/shared/src/index.ts --out typedocs",
    "build:main-doc": "npx typedoc packages/turbox/src/index.ts packages/turbox2d/src/index.ts packages/turbox3d/src/index.ts --out typedocs",
    "build:all": "pnpm run -r build:all",
    "publish:pre": "pnpm -r publish --dry-run --report-summary --no-git-checks --force",
    "publish": "pnpm -r publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/turbox3d/turbox.git"
  },
  "keywords": [
    "framework",
    "productivity application",
    "front-end"
  ],
  "author": "feifan <darknight3@vip.qq.com>",
  "license": "MIT",
  "peerDependencies": {
    "pixi.js": ">=5.3",
    "react": "16.x",
    "react-dom": "16.x",
    "three": "0.x"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.8.1",
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.5",
    "@babel/plugin-transform-modules-commonjs": "^7.23.3",
    "@babel/plugin-transform-runtime": "^7.23.4",
    "@babel/preset-env": "^7.23.5",
    "@babel/preset-react": "^7.23.3",
    "@babel/runtime": "^7.23.5",
    "@turbox3d/reactivity-react": "workspace:^",
    "@turbox3d/turbox": "workspace:^",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.20.55",
    "@types/react": "~16.14.52",
    "@types/react-dom": "~16.9.24",
    "@types/three": "^0.130.2",
    "@types/webpack-env": "^1.18.4",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "antd": "^4.24.15",
    "axios": "^0.20.0",
    "babel-loader": "^8.3.0",
    "cross-env": "^6.0.3",
    "css-loader": "^5.2.7",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^4.3.8",
    "jest": "^25.5.4",
    "less": "^4.2.0",
    "less-loader": "^7.3.0",
    "lint-staged": "^10.5.4",
    "lodash": "^4.17.21",
    "mobx": "^5.15.7",
    "pixi.js": "^5.3.12",
    "postcss-loader": "^4.3.0",
    "react": "~16.14.0",
    "react-dom": "~16.14.0",
    "three": "^0.131.3",
    "ts-jest": "^24.3.0",
    "ts-loader": "^7.0.5",
    "typedoc": "^0.21.10",
    "typescript": "^4.9.5",
    "url-loader": "^4.1.1",
    "vsce": "^1.103.1",
    "webpack": "^4.47.0",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run lint-tsc"
    }
  }
}
