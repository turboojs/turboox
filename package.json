{
  "name": "turbox",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "scripts": {
    "start": "webpack-dev-server --config ./demo/webpack.config.js",
    "build": "NODE_ENV=production webpack ./demo/src/main --mode production --config ./demo/webpack.config.js",
    "lint-tsc": "tsc --noEmit",
    "typedoc": "rm -rf typedocs && cross-env DOC_ENV=typedoc npx ./gen-doc.js",
    "changelog": "rm -rf ./docs/CHANGELOG.md && cross-env DOC_ENV=changelog npx ./gen-doc.js",
    "build:all": "pnpm run -r build:all && pnpm run typedoc",
    "git:cl": "git add . && git commit -m 'doc: add changelog' --no-verify && git push --all",
    "publish": "pnpm run build:all && pnpm changeset && pnpm changeset version && pnpm run changelog && pnpm run git:cl && pnpm changeset tag && pnpm -r publish",
    "publish:pre": "pnpm run build:all && pnpm -r publish --dry-run --report-summary --no-git-checks --force"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/turbox3d/turbox.git"
  },
  "keywords": [
    "framework",
    "productivity application",
    "front-end"
  ],
  "author": "feifan <darknight3@vip.qq.com>",
  "license": "MIT",
  "peerDependencies": {
    "pixi.js": ">=5.3",
    "react": ">=16",
    "react-dom": ">=16",
    "three": "0.x"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.8.1",
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.5",
    "@babel/plugin-transform-modules-commonjs": "^7.23.3",
    "@babel/plugin-transform-runtime": "^7.23.4",
    "@babel/preset-env": "^7.23.5",
    "@babel/preset-react": "^7.23.3",
    "@babel/runtime": "^7.23.5",
    "@changesets/cli": "^2.27.1",
    "@turbox3d/reactivity-react": "workspace:^",
    "@turbox3d/turbox": "workspace:^",
    "@turbox3d/turbox-dev-tool": "workspace:^",
    "@types/jest": "^24.9.1",
    "@types/node": "~16.18.68",
    "@types/react": "~17.0.71",
    "@types/react-dom": "~17.0.25",
    "@types/three": "^0.130.2",
    "@types/webpack-env": "^1.18.4",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "antd": "^4.24.15",
    "axios": "^0.20.0",
    "babel-loader": "^8.3.0",
    "cross-env": "^6.0.3",
    "css-loader": "^5.2.7",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^4.3.8",
    "jest": "^24.9.0",
    "less": "^4.2.0",
    "less-loader": "^7.3.0",
    "lint-staged": "^10.5.4",
    "lodash": "^4.17.21",
    "mobx": "^5.15.7",
    "pixi-projection": "^0.4.4",
    "pixi.js": "^5.3.12",
    "postcss-loader": "^4.3.0",
    "react": "~17.0.2",
    "react-dom": "~17.0.2",
    "react-router-dom": "^6.20.1",
    "style-loader": "^2.0.0",
    "three": "^0.131.3",
    "ts-jest": "^24.3.0",
    "ts-loader": "~8.4.0",
    "typedoc": "^0.25.4",
    "typescript": "4.6.4",
    "url-loader": "^4.1.1",
    "vsce": "^1.103.1",
    "webpack": "4.42.1",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "~3.3.12",
    "webpack-dev-server": "3.10.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run lint-tsc"
    }
  }
}
