{
  "name": "turbox",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "webpack-dev-server --config ./demo/webpack.config.js",
    "build": "NODE_ENV=production webpack ./demo/src/main --mode production --config ./demo/webpack.config.js",
    "lint-tsc": "tsc --noEmit",
    "build:doc": "npx typedoc packages/command-manager/src/index.ts packages/design-engine/src/index.ts packages/event-manager/src/index.ts packages/graphic-component-pixi/src/index.ts packages/graphic-component-three/src/index.ts packages/renderer-core/src/index.ts packages/renderer-pixi/src/index.ts packages/renderer-three/src/index.ts packages/math/src/index.ts packages/reactivity/src/index.ts packages/reactivity-react/src/index.ts packages/shared/src/index.ts --out typedocs",
    "build:doc-inner": "npx typedoc packages/app-plugin-manager/src/index.ts packages/command-manager/src/index.ts packages/design-engine/src/index.ts packages/event-manager/src/index.ts packages/graphic-component-pixi/src/index.ts packages/graphic-component-three/src/index.ts packages/renderer-core/src/index.ts packages/renderer-pixi/src/index.ts packages/renderer-three/src/index.ts packages/math/src/index.ts packages/production-engine/src/index.ts packages/reactivity/src/index.ts packages/reactivity-react/src/index.ts packages/shared/src/index.ts packages/turbox-dev-core/src/index.ts packages/web-ui/src/index.ts --out typedocs",
    "build:main-doc": "npx typedoc packages/turbox/src/index.ts packages/turbox3d/src/index.ts --out typedocs",
    "prerelease": "npx lerna version --no-push",
    "release": "npx lerna publish from-git --yes",
    "post-release": "git push --follow-tags origin",
    "pre-deploy": "npx lerna bootstrap && npx lerna run build:ts",
    "deploy": "npx lerna publish"
  },
  "tnpm": {
    "mode": "yarn"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/turbox3d/turbox.git"
  },
  "keywords": [
    "framework",
    "productivity application",
    "front-end"
  ],
  "author": "feifan <darknight3@vip.qq.com>",
  "license": "MIT",
  "peerDependencies": {
    "pixi.js": ">=5.3",
    "three": "0.x",
    "react": "16.x",
    "react-dom": "16.x"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.3.0",
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-transform-modules-commonjs": "^7.6.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.9.4",
    "@babel/runtime": "^7.6.3",
    "@turbox3d/reactivity-react": "^1.0.14",
    "@turbox3d/turbox3d": "^1.0.5",
    "@types/jest": "^24.0.19",
    "@types/node": "^12.11.5",
    "@types/react": "16.x",
    "@types/react-dom": "16.x",
    "@types/three": "^0.130.1",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "antd": "^4.10.0",
    "axios": "^0.20.0",
    "babel-loader": "^8.1.0",
    "cross-env": "^6.0.3",
    "css-loader": "^5.0.1",
    "eslint": "^7.12.1",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.0",
    "jest": "^25.3.0",
    "lerna": "^3.22.1",
    "less": "^4.0.0",
    "less-loader": "^7.2.1",
    "lint-staged": "^10.5.1",
    "lodash": "^4.17.15",
    "mobx": "^5.15.4",
    "pixi.js": "^5.3.3",
    "postcss-loader": "^4.1.0",
    "react": "16.x",
    "react-dom": "16.x",
    "three": "^0.131.3",
    "ts-jest": "^24.1.0",
    "ts-loader": "^7.0.0",
    "typedoc": "^0.21.4",
    "typescript": "^4.3.5",
    "url-loader": "^4.1.1",
    "vsce": "^1.68.0",
    "webpack": "^4.42.1",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.10.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint",
      "git add"
    ]
  },
